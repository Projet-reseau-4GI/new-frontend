    "components": {
        "schemas": {
            "DocumentAnalysisResponse": {
                "type": "object",
                "properties": {
                    "documentType": {
                        "type": "string"
                    },
                    "documentNumber": {
                        "type": "string",
                        "maxLength": 30,
                        "minLength": 5
                    },
                    "holderName": {
                        "type": "string"
                    },
                    "dateOfBirth": {
                        "type": "string",
                        "format": "date"
                    },
                    "issueDate": {
                        "type": "string",
                        "format": "date"
                    },
                    "expirationDate": {
                        "type": "string",
                        "format": "date"
                    },
                    "isValid": {
                        "type": "boolean"
                    },
                    "validationMessage": {
                        "type": "string"
                    },
                    "confidenceScore": {
                        "type": "number",
                        "format": "double"
                    },
                    "hasUncertainty": {
                        "type": "boolean"
                    },
                    "additionalFields": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "string"
                        }
                    },
                    "rawExtractedText": {
                        "type": "string"
                    }
                },
                "required": [
                    "documentType",
                    "holderName",
                    "isValid"
                ]
            },
            "DocumentEntity": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid"
                    },
                    "pieceType": {
                        "type": "string"
                    },
                    "fileName": {
                        "type": "string"
                    },
                    "fileSize": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "minioPath": {
                        "type": "string"
                    },
                    "backMinioPath": {
                        "type": "string"
                    },
                    "userId": {
                        "type": "string",
                        "format": "uuid"
                    },
                    "fileType": {
                        "type": "string"
                    },
                    "uploadDate": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "status": {
                        "type": "string"
                    }
                }
            },
            "VerifyEmailRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "code": {
                        "type": "string",
                        "maxLength": 6,
                        "minLength": 6
                    }
                },
                "required": [
                    "code",
                    "email"
                ]
            },
            "VerifyCodeRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "code": {
                        "type": "string",
                        "pattern": "\\d{6}"
                    }
                },
                "required": [
                    "code",
                    "email"
                ]
            },
            "ResetPasswordRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "code": {
                        "type": "string",
                        "pattern": "\\d{6}"
                    },
                    "newPassword": {
                        "type": "string",
                        "maxLength": 2147483647,
                        "minLength": 8,
                        "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]{8,}$"
                    }
                },
                "required": [
                    "code",
                    "email",
                    "newPassword"
                ]
            },
            "ResendVerificationRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    }
                },
                "required": [
                    "email"
                ]
            },
            "RegisterRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string",
                        "maxLength": 2147483647,
                        "minLength": 6
                    },
                    "firstName": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    }
                },
                "required": [
                    "email",
                    "firstName",
                    "lastName",
                    "password"
                ]
            },
            "LoginRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "required": [
                    "email",
                    "password"
                ]
            },
            "GoogleCallbackRequest": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    }
                }
            },
            "GoogleAuthResponse": {
                "type": "object",
                "properties": {
                    "token": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "newUser": {
                        "type": "boolean"
                    }
                }
            },
            "ForgotPasswordRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    }
                },
                "required": [
                    "email"
                ]
            },
            "AuthUrlResponse": {
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string"
                    }
                }
            }
        }
    }
}